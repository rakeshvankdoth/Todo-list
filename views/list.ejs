<%- include('header'); -%>
    <div class="box" id="heading"><h1>It's a <%= listtitle %>.</h1></div>
    <div class="box">
        <%for(var i = 0;i<newitem.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= newitem[i] %></p>
            </div>
            <%}%>
        <form class="item" action="/" method="post">
        <input type="text" name="Additem" placeholder="Add an item">
        
        <button type="submit" name="list" value="listtitle">+</button>
    </form>
</div>
<%- include('footer'); -%>